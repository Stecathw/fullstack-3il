<!DOCTYPE html>
<html>
  <head>
    <title>Contacts</title>
    <style>
      table {
        border-collapse: collapse;
        width: 100%;
      }
      th, td {
        text-align: left;
        padding: 8px;
      }
      th {
        background-color: #f2f2f2;
      }
      tr:nth-child(even) {
        background-color: #f2f2f2;
      }
    </style>
  </head>
  <body>
    <h1>Contacts</h1>
    <table>
      <tr>
        <th>ID</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Genre</th>
      </tr>
      <% contacts.forEach(contact => { %>
        <tr>
          <td><%= contact.id %></td>
          <td><%= contact.firstname %></td>
          <td><%= contact.lastname %></td>
          <td><%= contact.Genre.libelle %></td>
        </tr>
      <% }) %>
    </table>
  </body>
</html>
