<!DOCTYPE html>
<html>
  <head>
    <title>Tickets</title>
    <style>
      table {
        border-collapse: collapse;
        width: 100%;
      }
      th, td {
        text-align: left;
        padding: 8px;
      }
      th {
        background-color: #f2f2f2;
      }
      tr:nth-child(even) {
        background-color: #f2f2f2;
      }
    </style>
  </head>
  <body>
    <h1>Tickets</h1>
    <table>
      <tr>
        <th>ID</th>
        <th>Title</th>
        <th>Description</th>
        <th>Status</th>
      </tr>
      <% tickets.forEach(ticket => { %>
        <tr>
          <td><%= ticket.id %></td>
          <td><%= ticket.title %></td>
          <td><%= ticket.description %></td>
          <td><%= ticket.status %></td>
        </tr>
      <% }) %>
    </table>
  </body>
</html>
