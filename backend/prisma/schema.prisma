generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Ticket {
  id          Int          @id @default(autoincrement())
  title       String       @db.VarChar(100)
  description String       @default("")
  status      TicketStatus @default(A_FAIRE)
}

enum TicketStatus {
  A_FAIRE
  EN_COURS
  TERMINE
}
