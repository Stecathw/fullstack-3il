    "prisma": "^4.13.0"
  },
  "devDependencies": {
    "@prisma/client": "^4.13.0",
    "@types/express": "^4.17.17",
    "@types/node": "^18.16.3",
    "@types/supertest": "^2.0.12",
    "concurrently": "^8.0.1",
    "dotenv-cli": "^7.1.0",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
